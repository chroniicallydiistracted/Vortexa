{
  "name": "weather-web",
  "version": "0.2.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run --reporter=dot",
    "analyze": "vite build --mode=production && npx --yes source-map-explorer 'dist/assets/*.js' --html analyze.html",
    "grep:no-legacy": "git grep -n 'src/ui/Panel' -- web || true",
    "grep:no-hex": "git grep -nE '#[0-9a-fA-F]{3,8}\\b' -- web/src | grep -v 'src/theme' || true"
  },
  "dependencies": {
    "@mantine/charts": "^8.2.0",
    "@mantine/core": "^8.2.0",
    "@mantine/dates": "^8.2.0",
    "@mantine/hooks": "^8.2.0",
    "@mantine/modals": "^8.2.0",
    "@mantine/notifications": "^8.2.0",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.51.0",
    "@westfam/shared": "file:../services/shared",
    "cesium": "^1.118.0",
    "maplibre-gl": "^4.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.1.2",
    "use-debounce": "^10.0.5",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.16.11",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.3.4",
    "axe-core": "^4.10.0",
    "canvas": "^3.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.5.4",
    "vite": "^6.3.5",
    "vitest-axe": "^0.1.0"
  }
}
